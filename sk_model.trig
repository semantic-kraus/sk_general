  rdfs:isDefinedBy "https://cidoc-crm.org/html/cidoc_crm_v7.1.2.html"^^xsd:string .

<http://www.cidoc-crm.org/cidoc-crm/P144i_gained_member_by>
  a rdf:Property ;
  rdfs:label "gained member by"@en, "erwarb Mitglied durch"@de, "获得成员"@zh ;
  rdfs:domain <http://www.cidoc-crm.org/cidoc-crm/E74_Group> ;
