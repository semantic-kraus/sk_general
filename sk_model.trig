  owl:inverseOf <https://w3id.org/lso/intro/beta202304#R16_is_incorporated_in> ;
  rdfs:domain <https://w3id.org/lso/intro/beta202304#INT16_Segment> ;
  rdfs:range <http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object> ;
