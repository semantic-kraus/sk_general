  rdfs:range <https://w3id.org/lso/intro/beta202304#INT4_ReceptionalEntity> ;
  rdfs:comment "Links the specific INT2 Actualization of Feature to the (abstract concept of the) feature it actualizes, a INT4 Receptional Entity." ;
  rdfs:isDefinedBy "https://w3id.org/lso/intro/beta202304"^^xsd:string .

<https://w3id.org/lso/intro/beta202304#R17_feature_actualized_in>
  a owl:ObjectProperty ;
  owl:inverseOf <https://w3id.org/lso/intro/beta202304#R17_actualizes_feature> ;
